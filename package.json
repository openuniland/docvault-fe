{
  "name": "revise-fe",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@hookform/resolvers": "^3.0.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.121",
    "@mui/material": "^5.11.11",
    "@mui/styled-engine-sc": "^5.11.0",
    "@react-oauth/google": "^0.8.0",
    "@reduxjs/toolkit": "^1.9.2",
    "@tanstack/react-query": "^4.24.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tippyjs/react": "^4.2.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.13",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "eslint-config-airbnb": "^19.0.4",
    "husky": "^8.0.3",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "moment": "^2.29.4",
    "normalize.css": "^8.0.1",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^12.1.5",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "sanitize.css": "^13.0.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start:prod": "npm run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint src/**/*.css",
    "prettier": "prettier --write ./src/**/*.{js, jsx,ts,tsx,scss, css}",
    "prettier:check": "prettier --list-different ./src/**/*.{js, jsx,ts,tsx,scss, css}",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/react": "^18.0.30",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^10.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run lint-fix",
      "npm run prettier",
      "git add ."
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
